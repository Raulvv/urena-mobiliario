{
    "version": 2,
    "name": "urena-mobiliario",
    "builds": [
      { "src": "api/**/*.js", "use": "@now/node" },
      { "src": "public/*.html", "use": "@now/static" }
    ],
    "routes": [
      {
        "src": "/public/images/(.*)",
        "headers": {
          "cache-control": "s-maxage=31536000,immutable"
        },
        "dest": "/images/$1"
      },
      { "src": "/public/favicon.ico", "dest": "/favicon.ico" }
    ]
  }
